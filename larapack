#!/usr/bin/env php
<?php
require_once __DIR__ . '/vendor/autoload.php';

use Pragnesh\LaravelPackageHelper\Command\CreateConfigCommand;
use Pragnesh\LaravelPackageHelper\Command\MakeControllerCommand;
use Pragnesh\LaravelPackageHelper\Command\MakeTestCommand;
use Symfony\Component\Console\Application;

$app = new Application();

$app->addCommands([
	new CreateConfigCommand(),
	new MakeTestCommand(),
	new MakeControllerCommand(),
]);

$app->run();