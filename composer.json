{
  "name": "pragnesh/laravel-package-helper",
  "description": "Provide commands to create laravel package files.",
  "type": "library",
  "keywords": [
    "package-development",
    "package-helper",
    "laravel-package"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Pragnesh",
      "email": "pragnesh3107@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "symfony/console": "^6.1",
    "symfony/filesystem": "^6.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Pragnesh\\LaravelPackageHelper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pragnesh\\LaravelPackageHelper\\Tests\\": "tests/"
    }
  },
  "bin": [
    "larapack"
  ],
  "minimum-stability": "dev"
}
