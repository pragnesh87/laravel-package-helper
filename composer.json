{
  "name": "pragnesh/laravel-package-helper",
  "description": "Provide a list of commands to create the laravel package, similar to artisan commands.",
  "type": "library",
  "keywords": [
    "package-development",
    "package-helper",
    "laravel-package",
    "laravel",
    "package",
    "helper"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Pragnesh",
      "email": "pragnesh3107@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "symfony/console": "^6.2",
    "symfony/filesystem": "^6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10"
  },
  "autoload": {
    "psr-4": {
      "Pragnesh\\LaravelPackageHelper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pragnesh\\LaravelPackageHelper\\Tests\\": "tests/"
    }
  },
  "bin": [
    "larapack"
  ],
  "prefer-stable": true,
  "minimum-stability": "dev"
}
